import parsing

function DemoProgram(text) is
	array<string> strings = []
	array<double> numbers = []
	array<string> substringsSplitted = parsing.split("//")

end

function isExpression(text) : boolean is
	var i is 0
	var isCharPresented is false

	for i in 0..text.size loop
		if parsing.isChar(text[i]) then
			isCharPresented = true
		end
	end



end