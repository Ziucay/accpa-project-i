function multiplication (a : double, b : double) : auto is
    return a * b
end

function factorial (a : int) : auto is
    if a < 1 then
        return 1
    end

    return factorial(a - 1) * a
end

function pow (a: auto, power: int) : auto is
    if power < 2
        return a
    end

    return pow(a * a, power - 1)
end